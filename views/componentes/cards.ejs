    <div class="content">
        <h1 class="painel">PAINEL:</h1>
        <div class="cards-container">
        <% if (user.isBaned===true) { %>
              <!--  BLOQUEADO-->
            <div class="card">
                <i class="fas fa-lock"></i>
                <div>
                    <h3>Conta Bloqueada</h3>
                    <p>Você esta impossibilitado(a) de utilizar o site no momento</p>
                </div>
            </div>
          <% } %>
          <% if (user.isPremium===true) { %>
              <!--usuario premium-->
            <div class="card">
              <i class="fa fa-crown"></i>
            <div>
              <h3>Usuários Plus</h3>
               <p>@<%= user.username %> Agora você pode personalizar sua key!</p>
            </div>
            </div>
            <% } else { %>
              <!-- NICK DO USUARIO -->
            <div class="card">
                <i class="fas fa-user"></i>
                <div>
                    <h3>Usuário</h3>
                    <p>@<%= user.username %></p>
                </div>
            </div>
              <% } %>

              <!-- SALDO -->
            <div class="card">
                <i class="fas fa-wallet"></i>
                <div>
                    <h3>Saldo</h3>
                    <p>♧ <%= user.saldo %></p>
                </div>
            </div>
            <% if (user.isAdm) { %>
              <!-- ADM -->
            <div class="card">
                <i class="fas fa-cog"></i>
                <div>
                    <h3>Administração</h3>
                    <p>Use a nova opção que apareceu no menu lateral! </p>
                </div>
            </div>
            <% } else { %>
            <% } %>
              <!-- KEY -->
            <div class="card">
               <i class="fas fa-key"></i>
               <div>
                 <h3>Apikey/Chave</h3>
                 <p><%= user.key %></p>
               </div>
            </div>
              <!--Novidades-->
            <!--<div class="card">
                <i class="fas fa-bullhorn"></i>
                <div>
                    <h3>Novidades</h3>
                    <p>Confira as últimas atualizações!</p>
                </div>
            </div>-->
        </div>
    </div>